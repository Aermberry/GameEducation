				<tween:To duration="250"/>
				<tween:To duration="250">
					<tween:props>
						<e:Object y="{1280}"/>
					</tween:props>
				</tween:To>
			</tween:TweenItem>
		</tween:TweenGroup>
		<tween:TweenGroup id="completeGameTweenGroup">
			<tween:TweenItem target="{compeletText}">
				<tween:Set>
				<tween:props>
					<e:Object alpha="{1}" y="{0}"/>
				</tween:props>
				</tween:Set>
				<tween:To duration="250">
				<tween:props>
					<e:Object y="{416}"/>
				</tween:props>
				</tween:To>
				<tween:To duration="250">
