				</tween:Set>
			</tween:TweenItem>
		</tween:TweenGroup>
		<tween:TweenGroup id="tipsTweenGroup">
			<tween:TweenItem target="{requestText}">
				<tween:Set>
					<tween:props>
						<e:Object alpha="{0}"/>
